	// Used for bobbing hair, chest armor, and center of rotation.
	"walkBob" : [0, 0, 0, 0, 0, 0, 0, 0],
	"runBob" : [0, 0, 1, 1, 0, 0, -1, -1],
	"jumpBob" : 0,

	"swimBob" : [0, -1, -2, -1, 0, 0, 0],

	"frontArmRotationCenter" : [20, -3],

	"duckOffset" : 0,
	"headDuckOffset" : [0, 0],

	"sitOffset" : 0,
	"layOffset" : -7,
	"headSitOffset" : [0, 0],
	"headLayOffset" : [0, 0],

	"recoilOffset" : [-1, 0],

	// Walking sequence for front and back arms
	"armWalkSeq" : [2, 1, 2, 3, 4, 5, 4, 3],

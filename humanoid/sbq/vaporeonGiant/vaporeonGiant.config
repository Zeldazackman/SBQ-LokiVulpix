	// would otherwise be not at a whole pixel boundary.
	"globalOffset" : [0, 0],

	"headRunOffset" : [0, 0],
	"headSwimOffset" : [0, 0],

	// Offset for hair and chest when running or falling
	"runFallOffset" : 0,
